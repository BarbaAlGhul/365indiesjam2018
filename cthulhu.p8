function _init()
    cls()
end

function _update()
	movecursor()
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
	drawplaces()
	spr(hero.sprite,hero.x,hero.y)
 print(reino,97,11,0)
	print(vila,33,58,0)
	print(tenda,11,98,0)
	selecthero()
	spr(pc.sprite,pc.x,pc.y)
 dialog(intro,2)
 options()
 countturn()
 
end

function drawplaces()
	if not burnrei then
		spr(32,rei.sp1x,rei.sp1y)
		spr(33,rei.sp2x,rei.sp2y)
		spr(48,rei.sp3x,rei.sp3y)
		spr(49,rei.sp4x,rei.sp4y)
	else
	end
	if not burnvil then
		spr(17,vil.sp1x,vil.sp1y)
		spr(17,vil.sp2x,vil.sp2y)
	end
	if not burnten then
		spr(18,ten.sp1x,ten.sp1y)
		spr(18,ten.sp2x,ten.sp2y)
		spr(18,ten.sp3x,ten.sp3y)
	end
end

function movecursor()
	if not dl then
		if btn(0) then pc.x-=1	end
 	if btn(1) then	pc.x+=1 end
 	if btn(2) then pc.y-=1 end
 	if btn(3) then pc.y+=1 end
	end
end

function selecthero()
	for i=hero.x,hero.x+8 do
		for j=hero.y,hero.y-8,-1 do
			if i==pc.x and j==pc.y then
				spr(2,hero.x,hero.y)
				if btnp(4) then
					optmenu=true
			 end
			end
		end
	end
end

function dialog(text,lines)
	if dl then
		for i=0,lines-1 do
			rectfill(dbox.x0,dbox.y0+i*8,dbox.x1,dbox.y1+i*8,0)
		end
		for i=0, count(text)-1 do
			print(text[i+1],dbox.x0+8,dbox.y0+2+(i*8),7)
		end
		if btnp(4) then	dl=false	end
	end
end

function options()
	if optmenu then
		dl=true
		dialog(menutext,4)
		spr(menusel.sprite,menusel.x,menusel.y)
		if selopt < 1 then selopt=1
		elseif selopt > 4 then selopt=4
		else
			if btnp(2) then
			 selopt-=1
			 menusel.y-=8
			 if menusel.y<72 then menusel.y=72 end
			elseif btnp(3) then
			 selopt+=1
			 menusel.y+=8
			 if menusel.y>96 then menusel.y=96 end
			end
		end
		if btnp(4) then
				if selopt==1 then
					reinof()
				elseif selopt==2 then
					vilaf()
				elseif selopt==3 then
					tendaf()
				elseif selopt==4 then
					optmenu=false
					selopt=1
					menusel.y=72
				end
		end
	end
end

function countturn()
	print("turno atual: "..tostr(currentturn),72,120,0)
end

function cthulhu(cth)
end
-->8
function reinof()
	plturn=false
	if plturn then
		currentturn+=1
	end
end

function vilaf()
	plturn=false
	if plturn then
		currentturn+=1
	end
end

function tendaf()
	plturn=false
	if plturn then
		currentturn+=1
	end
end
